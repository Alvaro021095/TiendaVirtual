<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="CliCtlPortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:9090/cliente" binding="basicHttpBinding"
                bindingConfiguration="CliCtlPortBinding" contract="ServicioCliente.ClienteCtl"
                name="CliCtlPort" />
        </client>
    </system.serviceModel>
</configuration>